#projects
  .container-fluid
    .row
      each val, key in projects
        .project.col-md-4.col-sm-6.col-xs-12(class="#{ key }")
          if val.web
            a(href="projects/#{ key }")
              .appname= val.name
              .appdescription= val.description
              if val.soon
                .webimage.comingsoon
              else
                .webimage
          else
            a(href="projects/#{ key }")
              if val.soon
                .iconimage.comingsoon
              else
                .iconimage
              .appname= val.name
              .appdescription= val.description
